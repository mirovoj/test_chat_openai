    {
        "id": "8434c9e7.baf1e8",
        "type": "function",
        "z": "93bb1b11.b35458",
        "name": "Doplnkove chladenie alebo kurenie",
        "func": "var delta = global.get('Priestor' ) - global.get('Konvektor_1');\nif (( global.get('Priestor') > 25 ) && ( global.get('Vonku') > 25 ) && delta >= 1.5) {\n    msg.payload = true ;\n    // zapni aj dolpnkove chladenie\n    }\n    else if (( global.get('Zimna') < 23 ) && ( global.get('Vonku') < 15 ) && ( global.get('Konvektor_1') > 25 )) {\n        // zapni ventilator ak je teplota v zimnej nižśia ako 26 °C inak nekúr.\n        msg.payload = false ;\n        if ( global.get('Zimna') < 26 ) {\n            msg.payload = true \n            \n        } \n    }\n    else  msg.payload = false ;\nreturn msg;\n",
        "outputs": 1,
        "noerr": 0,
        "x": 360,
        "y": 340,
        "wires": [
