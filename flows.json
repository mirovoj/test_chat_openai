    {
        "id": "7a7b992a.cb2af",
        "type": "comment",
        "z": "93bb1b11.b35458",
        "name": "chladenie studňou",
        "info": "// takto snimam teploty v Datalog\n\n\n\n  var err = 85;\n//msg.timestamp = new Date() ;\nvar teplota = msg.payload;\nvar msg1 = { payload: msg.payload.length };\n\nif (msg.payload != err){\n    if (msg.topic == \"28.99E9A0010000/temperature\")\n    { \n    msg1.payload  =  [{ measurement: \"home\",\n        fields: { value: teplota},\n        tags: { location: \"privod\"}\n        }\n        ];\n    }\n    \n    else if (msg.topic == \"10.10F30C020800/temperature\"){ \n        msg1.payload = [{ measurement: \"home\",\n        fields: { value: teplota},\n        tags: {location:\"vonku\"}\n        }\n        ];\n    }\n    \n    else if (msg.topic == \"10.9BEC0C020800/temperature\"){ \n        msg1.payload = [{measurement: \"home\",\n        fields: { value: teplota},\n        tags: { location:\"priestor\"}\n        }\n        ];\n        var temp = parseFloat ( teplota);\n        global.set(\"Priestor\", temp);\n    }\n    \n    else if (msg.topic == \"28.2002B7020000/temperature\"){ \n        msg1.payload = [{ measurement: \"home\",\n        fields: { value: teplota},\n        tags:{ location:\"spiatocka\"}\n        }\n        ];\n    }\n   \n    else if (msg.topic == \"28.04D2DA020000/temperature\"){ \n        msg1.payload = [{ measurement: \"home\",\n        fields: { value: teplota},\n        tags:{ location:\"cidlo_2804D2\"}\n        }\n        ];\n    }\n    \n    else if (msg.topic == \"28.34F8A0010000/temperature\"){ \n        msg1.payload = [{ measurement: \"home\",\n        fields: { value: teplota},\n        tags:{ location:\"cidlo2834F8\"}\n        }\n        ];\n    }\n    \n    else if (msg.topic == \"28.9A3302020000/temperature\"){ \n        msg1.payload = [{ measurement: \"home\",\n        fields: { value: teplota},\n        tags:{ location:\"cidlo289A33\"}\n        }\n        ];\n    }\n    \n    else if (msg.topic == \"28.055C02020000/temperature\"){ \n        msg1.payload = [{ measurement: \"home\",\n        fields: { value: teplota},\n        tags:{ location:\"cidlo28055C\"}\n        }\n        ];\n    }\n    \n    else if (msg.topic == \"28.4D01A1010000/temperature\"){ \n        msg1.payload = [{ measurement: \"home\",\n        fields: { value: teplota},\n        tags:{ location:\"cidlo284D01\"}\n        }\n        ];\n    }\n    \n    else  return null;\n}\n\nelse  return null;\n\n//return [msg];\n//msg.timestamp = new Date() ;\nreturn msg1;\n\n\n\n\n//10.10F30C020800/temperature\n//10.9BEC0C020800/temperature\n//28.2002B7020000/temperature\n//28.04D2DA020000/temperature\n//28.34F8A0010000/temperature\n//28.9A3302020000/temperature\n//28.99E9A0010000/temperature\n//28.055C02020000/temperature\n//28.4D01A1010000/temperature",
        "x": 90,
        "y": 40,
        "wires": []
    },
